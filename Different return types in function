#include<stdio.h>
void goodMorning();
void goodAfternoon();
void goodNight();

int main()
{
    goodMorning();
    return 0;
}
void goodMorning()
{
printf("Good Morning Shiwani\n");
goodAfternoon();
}
void goodAfternoon()
{
    printf("Good Afternoon Shiwani\n");
    goodNight();
}
void goodNight()
{
    printf("Good Night Shiwani\n");

}
