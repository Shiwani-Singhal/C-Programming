#include<stdio.h>
float main()
{
   float a = 3.2;
   float b = 3.5;
   printf("sum of a and b is: %f\n", a+b);
   printf("difference of a and b is: %f\n", a-b);
   printf("product of a and b is: %f\n",a*b);
   printf("a divides b is: %f\n",a/b);
   printf("a modulas b is : %f\n",a%b);

   return 0;
}
